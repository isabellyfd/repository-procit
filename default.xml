<form name="TD">
     <page number="1">
      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>contributor</dc-element>
       <dc-qualifier>author</dc-qualifier>
       <repeatable>true</repeatable>
       <label>Autor(es)*</label>
       <input-type>name</input-type>
       <hint>Digite o(s) nome(s) do(s) autor(es) da tese ou dissertação.</hint>
       <required>Digite o nome de pelo menos um autor.</required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>contributor</dc-element>
       <dc-qualifier>advisor</dc-qualifier>
       <repeatable></repeatable>
       <label>Orientador*</label>
       <input-type>name</input-type>
       <hint>Digite o nome do orientador da tese ou dissertação.</hint>
       <required>Digite o nome do orientador.</required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>contributor</dc-element>
       <dc-qualifier>advisor-co</dc-qualifier>
       <repeatable></repeatable>
       <label>Coorientador</label>
       <input-type>name</input-type>
       <hint>Digite o nome do coorientador da tese ou dissertação.</hint>
       <required></required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>identifier</dc-element>
       <dc-qualifier>citation</dc-qualifier>
       <repeatable></repeatable>
       <label>Citação</label>
       <input-type>textarea</input-type>
       <hint>Citação do autor.</hint>
       <required></required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>title</dc-element>
       <dc-qualifier></dc-qualifier>
       <repeatable></repeatable>
       <label>Título*</label>
       <input-type>onebox</input-type>
       <hint>Digite o título da tese ou dissertação.</hint>
       <required>Digite o título da tese ou dissertação.</required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>title</dc-element>
       <dc-qualifier>alternative</dc-qualifier>
       <repeatable></repeatable>
       <label>Título Alternativo</label>
       <input-type>onebox</input-type>
       <hint>Digite o título da tese ou dissertação em outro idioma.</hint>
       <required></required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>type</dc-element>
       <dc-qualifier></dc-qualifier>
       <repeatable>false</repeatable>
       <label>Tipo de Documento:</label>
       <input-type value-pairs-name="typ2">dropdown</input-type>
       <hint>Selecione o tipo</hint>
       <required>Por favor, selecione o tipo.</required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>description</dc-element>
       <dc-qualifier>abstract</dc-qualifier>
       <repeatable>false</repeatable>
       <label>Resumo*</label>
       <input-type>textarea</input-type>
       <hint>Digite o resumo (português) da tese ou dissertação.</hint>
       <required>Digite o resumo.</required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>description</dc-element>
       <dc-qualifier>abstractx</dc-qualifier>
       <repeatable></repeatable>
       <label>Resumo (Outro idioma)</label>
       <input-type>textarea</input-type>
       <hint>Digite o resumo (abstract) da tese ou dissertação em outro idioma.</hint>
       <required></required>
      </field>
      
      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>subject</dc-element>
       <dc-qualifier></dc-qualifier>
       <repeatable>true</repeatable>
       <label>Palavras-chave</label>
       <input-type>onebox</input-type>
       <hint>Digite as palavras chave da tese ou dissertação, uma a uma.</hint>
       <required></required>
      </field>

      <field>
       <dc-schema>dc</dc-schema>
       <dc-element>language</dc-element>
       <dc-qualifier>iso</dc-qualifier>
       <repeatable></repeatable>
       <label>Idioma</label>
       <input-type>onebox</input-type>
       <hint>Digite qual idioma predomina na tese ou dissertação.</hint>
       <required></required>
      </field>
     </page>

